<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Intel"
        tools:targetApi="31">
        <activity
            android:name=".Products.Details"
            android:exported="false" />
        <activity
            android:name=".Cart.CartDisplay"
            android:exported="false" />
        <activity
            android:name=".Products.ProductDetails"
            android:exported="false" />
        <activity
            android:name=".Products.ProductViewHolder"
            android:exported="false" />
        <activity
            android:name=".Products.ProductList"
            android:exported="false" />
        <activity
            android:name=".credentials.emailVerification"
            android:exported="false" />
        <activity
            android:name=".Products.ProductsPage"
            android:exported="false" />
        <activity
            android:name=".credentials.LoginPage"
            android:exported="false" />
        <activity
            android:name=".credentials.CreatePage"
            android:exported="false" />
        <activity
            android:name=".credentials.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>